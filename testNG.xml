<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<!--<?xml version="1.0" encoding="UTF-8"?>-->
<suite name="Public APIs Test Suite" parallel="false">
    <test name="Smoke Tests">
        <classes>
            <class name="smoke.SmokeTest"/>
        </classes>
    </test>
    <test name="Functional Tests">
        <classes>
            <class name="functional.FunctionalTest"/>
        </classes>
    </test>
    <test name="Performance Tests">
        <classes>
            <class name="performance.PerformanceTest"/>
        </classes>
    </test>
    <test name="Regression Tests">
        <classes>
            <class name="regression.RegressionTest"/>
        </classes>
    </test>
</suite>
